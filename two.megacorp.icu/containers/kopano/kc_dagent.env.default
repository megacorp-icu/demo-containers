KC_DAGENT_server_socket=file:///var/run/kopano/server.sock
KC_DAGENT_run_as_user=kopano
KC_DAGENT_run_as_group=kopano
KC_DAGENT_coredump_enabled=systemdefault
KC_DAGENT_process_model=thread
KC_DAGENT_sslkey_file=
KC_DAGENT_sslkey_pass=
KC_DAGENT_lmtp_listen=*:2003
KC_DAGENT_lmtp_max_threads=20
KC_DAGENT_spam_header_name=X-Spam-Status
KC_DAGENT_spam_header_value=Yes,
KC_DAGENT_log_method=file
KC_DAGENT_log_file=-
KC_DAGENT_log_timestamp=1
KC_DAGENT_log_buffer_size=0
KC_DAGENT_log_level=3
KC_DAGENT_log_raw_message=error
KC_DAGENT_log_raw_message_path=/var/lib/kopano
KC_DAGENT_archive_on_delivery=no
KC_DAGENT_mr_autoaccepter=/usr/sbin/kopano-mr-accept
KC_DAGENT_mr_autoprocessor=/usr/bin/kopano-mr-process
KC_DAGENT_autoresponder=/usr/sbin/kopano-autorespond
KC_DAGENT_inhibit_forward_headers=X-Kopano-Vacation Auto-Submitted
KC_DAGENT_inhibit_auto_reply_headers=X-Kopano-Vacation Auto-Submitted Precedence List-Id List-Help List-Subscribe List-Unsubscribe List-Post List-Owner List-Archive "X-Spam-Flag: YES" "X-Is-Junk: YES" X-AMAZON* X-LinkedIn*
KC_DAGENT_plugin_enabled=no
KC_DAGENT_plugin_manager_path=/usr/share/kopano-dagent/python
KC_DAGENT_plugin_path=/var/lib/kopano/dagent/plugins
KC_DAGENT_default_charset=us-ascii
KC_DAGENT_set_rule_headers=yes
KC_DAGENT_no_double_forward=yes
KC_DAGENT_forward_whitelist_domains=*
KC_DAGENT_forward_whitelist_domains_file=
KC_DAGENT_forward_whitelist_domain_subject=REJECT: %subject not forwarded (administratively blocked)
KC_DAGENT_forward_whitelist_domain_message=The Kopano mail system has rejected your request to forward your e-mail with subject %subject (via mail filters) to %sender: the operation is not permitted.\n\nRemove the rule or contact your administrator about the forward_whitelist_domains setting.
KC_DAGENT_forward_whitelist_domain_message_file=
KC_DAGENT_unknown_charset_substitution=
KC_DAGENT_indexed_headers=
KC_DAGENT_statsclient_url=unix:/var/run/kopano/statsd.sock
KC_DAGENT_statsclient_interval=0
KC_DAGENT_statsclient_ssl_verify=yes
KC_DAGENT_mail_conversion_detail=no
